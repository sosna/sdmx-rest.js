{
  "name": "sdmx-rest",
  "description": "The SDMX RESTful API for JavaScript",
  "main": "dist/index.js",
  "scripts": {
    "prebuild": "rm -rf dist && mkdir dist",
    "build": "browserify -t coffeeify --extension='.coffee' src/index.coffee > dist/index.js",
    "commit": "git-cz",
    "check-coverage": "istanbul check-coverage --statements 100 --branches 100 --functions 100 --lines 100",
    "report-coverage": "cat ./coverage/lcov.info | codecov",
    "test": "mocha --recursive --compilers coffee:coffee-script/register",
    "test:single": "istanbul cover -x **/*.test.coffee _mocha -- --recursive --compilers coffee:coffee-script/register --require coffee-coverage/register-istanbul",
    "semantic-release": "semantic-release pre && npm publish && semantic-release post",
    "lint": "coffeelint src"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/sosna/sdmx-rest4js.git"
  },
  "keywords": [
    "sdmx",
    "sdmx restful web services"
  ],
  "author": "Xavier Sosnovsky <xso@sosna.ws>",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/sosna/sdmx-rest4js/issues"
  },
  "homepage": "https://github.com/sosna/sdmx-rest4js#readme",
  "devDependencies": {
    "browserify": "13.0.0",
    "chai": "3.5.0",
    "codecov.io": "0.1.6",
    "coffee-coverage": "1.0.1",
    "coffee-script": "1.10.0",
    "coffeeify": "2.0.1",
    "coffeelint": "1.14.2",
    "commitizen": "2.5.0",
    "cz-conventional-changelog": "1.1.5",
    "ghooks": "1.0.3",
    "istanbul": "0.4.2",
    "mocha": "2.4.5",
    "semantic-release": "^4.3.5"
  },
  "config": {
    "commitizen": {
      "path": "node_modules/cz-conventional-changelog"
    },
    "ghooks": {
      "pre-commit": "npm run test:single && npm run check-coverage && npm run lint"
    }
  },
  "dependencies": {}
}
