{
  "name": "sdmx-rest",
  "description": "The SDMX RESTful API for JavaScript",
  "version": "1.0.0",
  "main": "./lib/index.js",
  "scripts": {
    "prebuild": "rm -rf lib && mkdir lib",
    "build": "./node_modules/.bin/coffee -co lib src/",
    "commit": "git-cz",
    "check-coverage": "istanbul check-coverage --statements 100 --branches 100 --functions 100 --lines 100",
    "report-coverage": "cat ./coverage/lcov.info | codecov",
    "test": "mocha -t 500 --recursive --compilers coffee:coffee-script/register",
    "test:watch": "mocha -t 500 -w --recursive --compilers coffee:coffee-script/register",
    "test:single": "istanbul cover _mocha -- --recursive --compilers coffee:coffee-script/register --require coffee-coverage/register-istanbul",
    "prepublish": "npm build",
    "semantic-release": "semantic-release pre && npm publish && semantic-release post",
    "lint": "coffeelint src"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/sosna/sdmx-rest4js.git"
  },
  "keywords": [
    "sdmx",
    "sdmx restful web services"
  ],
  "author": "Xavier Sosnovsky <xso@sosna.ws>",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/sosna/sdmx-rest4js/issues"
  },
  "homepage": "https://github.com/sosna/sdmx-rest4js#readme",
  "devDependencies": {
    "chai": "3.5.0",
    "chai-as-promised": "5.3.0",
    "codecov.io": "0.1.6",
    "coffee-coverage": "1.0.1",
    "coffee-script": "1.10.0",
    "coffeeify": "2.0.1",
    "coffeelint": "1.15.7",
    "commitizen": "2.8.1",
    "cz-conventional-changelog": "1.1.6",
    "ghooks": "1.2.1",
    "istanbul": "0.4.3",
    "mocha": "2.4.5",
    "nock": "8.0.0",
    "semantic-release": "6.2.1"
  },
  "config": {
    "commitizen": {
      "path": "node_modules/cz-conventional-changelog"
    },
    "ghooks": {
      "pre-commit": "npm run test:single && npm run check-coverage && npm run lint"
    }
  },
  "dependencies": {
    "es6-promise": "3.1.2",
    "isomorphic-fetch": "2.2.1"
  }
}
