{
  "name": "sdmx-rest",
  "engines": {
    "node": ">=6"
  },
  "description": "SDMX REST API client for JavaScript",
  "version": "2.11.0",
  "main": "./lib/index.js",
  "scripts": {
    "prebuild": "rm -rf lib && mkdir lib",
    "build": "coffee -co lib src/",
    "commit": "git-cz",
    "check-coverage": "istanbul check-coverage --statements 100 --branches 100 --functions 100 --lines 100",
    "report-coverage": "cat ./coverage/lcov.info | codecov",
    "test": "mocha -t 500 --recursive --compilers coffee:coffeescript/register",
    "test:watch": "mocha -t 500 -w --recursive --compilers coffee:coffeescript/register",
    "test:single": "istanbul cover _mocha -- --recursive --compilers coffee:coffeescript/register --require coffee-coverage/register-istanbul",
    "prepublish": "npm build",
    "semantic-release": "semantic-release pre && npm publish && semantic-release post",
    "lint": "coffeelint src"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/sosna/sdmx-rest4js.git"
  },
  "keywords": [
    "sdmx",
    "sdmx restful web services"
  ],
  "author": "Xavier Sosnovsky <xso@sosna.ws>",
  "contributors": [
    {
      "name": "Xavier Sosnovsky",
      "email": null,
      "url": "https://github.com/sosna",
      "contributions": 116,
      "additions": 5727,
      "deletions": 3071,
      "hireable": null
    },
    {
      "name": "Leroy S.",
      "email": null,
      "url": "https://github.com/Tzaphkiel",
      "contributions": 2,
      "additions": 21,
      "deletions": 1,
      "hireable": null
    }
  ],
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/sosna/sdmx-rest4js/issues"
  },
  "homepage": "https://github.com/sosna/sdmx-rest4js#readme",
  "devDependencies": {
    "chai": "4.2.0",
    "chai-as-promised": "7.1.1",
    "codecov.io": "0.1.6",
    "coffee-coverage": "3.0.0",
    "coffeeify": "3.0.1",
    "coffeelint": "2.1.0",
    "coffeescript": "2.1.1",
    "commitizen": "2.10.1",
    "cz-conventional-changelog": "2.1.0",
    "husky": "0.14.3",
    "istanbul": "0.4.5",
    "mocha": "5.2.0",
    "nock": "9.6.1",
    "semantic-release": "15.9.8"
  },
  "config": {
    "commitizen": {
      "path": "node_modules/cz-conventional-changelog"
    },
    "pre-commit": "npm run test:single && npm run check-coverage && npm run lint"
  },
  "dependencies": {
    "es6-promise": "4.2.5",
    "isomorphic-fetch": "2.2.1"
  }
}
